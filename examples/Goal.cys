// int = i64 = -2^64 / 2 --> 2^64 / 2 - 1
// float = f64
// str = std::string

void main(args: str[]) {
    println("Hello World!");

    const abc: int = 58;
    var xyz: int = 100;
    xyz -= abc;
    println("Is the answer " + xyz + "?");

    var ijk: int = 99;
    while ijk > 0 {
        print(ijk);
        ijk--;
    }
    println();

    println((4 + 5) / 2 - 1 * 2);

    const iii: int = 123;
    switch iii {
        1 {
            println("One");
        }
        // RANGE IDENTIFIER UNCLEAR
        2..5 {
            println("Two --> Five");
        }
        6, 7, 8 {
            println("Six or Seven or Eight");
        }
        ? {
            println("Unknown");
        }
    }

    // RANGE IDENTIFIER UNCLEAR + DEFINITION UNCLEAR
    for var l: int in 0..99 {
        println(l);
    }
}

